ALGORITHM insertSort
VAR
    tab : ARRAY_OF INTEGER[];
    j,i : INTEGER;
    key : INTEGER;
BEGIN
    read(tab)
    FOR i FROM 1 TO tab.length - 1 STEP 1  DO
        key := tab[i];
        j := i - 1;
        WHILE (j>=0 AND tab[j]> key) DO
            tab[j+1] := tab[j];
            j := j - 1;
        END_WHILE
        tab[j+1] := key;
    END_FOR
    write("sorted array is : "tab")
END